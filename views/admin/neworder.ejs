        <!-- order box  -->
        <div class="order-create-box mx-xl-4 mx-xxl-5 mx-3 pb-3 pt-2 pt-xl-3">
            <form class="product-form" action="/admin/add-product" method="POST" enctype="multipart/form-data">
                <div class="orderNo align-items-center w-50 me-3 me-xxl-5">
                                                <label for=" title">Pickup</label>
                    <input type="text" name="title" id="title">
                </div>
                <br>
                <hr>
                <div class="orderNo align-items-center w-50">
                    <label for="imageUrl">Location Picture </label>
                    <input type="text" name="imageUrl" id="imageUrl">
                </div>
                <br>
                <hr>
                <div class="orderNo align-items-center w-50 me-3 me-xxl-5">
                    <label for="price">kilometer traveled</label>
                    <input type="number" name="price" id="price" step="0.01">
                </div>
                <hr>
                <div class="orderNo align-items-center w-50 me-3 me-xxl-5">
                    <label for="pdf">Select Pdf</label>
                    <input type="file" name="pdf" id="pdf">
                </div>
                <hr>
                <div class="orderNo align-items-center w-50">
                    <label for="description">Leave Comment</label>
                    <textarea name="description" id="description" rows="5"></textarea>
                </div>
                <br>
                <hr>
                <div class="save-btn position-absolute">
                    <button class="btn fw-normal text-white px-4 py-3" type="submit">Save Changes</button>

                </div>


            </form>


        </div>

        </div>

